			<<else>>
				<<edenicon>><<link [[에덴의 오두막 (0:10)|Eden Clearing]]>><<pass 10>><</link>>
			<</if>>
			<br>
		<</if>>
		<<if $syndromewolves gte 1>>
			<<if $caveHumanPregnancyDiscovered isnot 1 or !playerIsPregnant()>>
				<<wolfcaveicon "entrance">><<link [[늑대 동굴 (0:10)|Wolf Cave Clearing]]>><<pass 10>><</link>>
				<br>
			<<else>>
